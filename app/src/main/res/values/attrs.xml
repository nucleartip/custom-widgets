<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BarGraph">
        <attr name="barColor" format="color" />
        <attr name="fillType" format="enum">
            <enum name="hollow" value="1" />
            <enum name="solid" value="2" />
        </attr>
        <attr name="strokeWidth" format="dimension" />
        <attr name="drawGuidelines" format="boolean" />
        <attr name="guidelinesColor" format="color" />
        <attr name="indicatorTextColor" format="color" />
        <attr name="indicatorTextSize" format="dimension" />
        <attr name="label_guideline_spacing" format="dimension"/>
        <attr name="bar_width" format="dimension"/>
    </declare-styleable>
</resources>